<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta name="keywords" content="" />
<meta name="description" content="" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<title>My page!</title>
<link href="http://fonts.googleapis.com/css?family=Arvo" rel="stylesheet" type="text/css" />
<link href="http://fonts.googleapis.com/css?family=Coda:400,800" rel="stylesheet" type="text/css" />
<link href="style.css" rel="stylesheet" type="text/css" media="screen" />
</head>
    <body>
    <div id="wrapper"> 
	<!-- end #header -->
	<div id="page-bgtop"></div>
	<div id="page">
		<div><img src="/images/marks.jpeg" width="920" height="500" alt="" /></div>
		<div id="content">
			<div class="post">
				<h2 class="title"><a href="#">Myform </a></h2>
				
				<div style="clear: both;">&nbsp;</div>
				<div class="entry">
                <a href='/sdetail/add'>Add records</a>
                <% if(db_rows_array.length > 0){ %>
                    <table border='1'>
                        <tr>
                            <th>Enrollment</th>
                            <th>Name</th>
                            <th>Semester</th>
                            <th>Division</th>
                            <th>Action</th>
                        </tr>
                        <% for(var i=0; i<db_rows_array.length;i++){ %>
                            <tr>
                                <td><%= db_rows_array[i].student_enr %></td>
                                <td><%= db_rows_array[i].student_name %></td>
                                <td><%= db_rows_array[i].student_sem %></td>
                                <td><%= db_rows_array[i].student_div %></td>
                                <td><a href='/sdetail/edit/<%= db_rows_array[i].student_enr %>'> Edit </a> | <a href='/sdetail/delete/<%= db_rows_array[i].student_enr %>'>Delete </a></td>
                            </tr>
                        <%}%>
                    </table>

                    <% } else{ %>
                        No records found
                        <% } %>
                    </div>
                </div>
                <div style="clear: both;">&nbsp;</div>
            </div>
            <!-- end #content -->
            

            <div style="clear: both;">&nbsp;</div>
    </div>
    <div id="page-bgbtm"></div>
    <!-- end #page -->
    </body>

</html>